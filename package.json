{
  "name": "sakumu",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "comment": "if you are using mac device, change the command from 'SET ENVFILE=.env && react-native run android/ios' to 'ENVFILE=.env react-native run android/ios'",
    "android": "react-native run-android",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:debug": "ENVFILE=.env.debug react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 15\"",
    "ios:staging": "SET ENVFILE=.env.staging && react-native run-ios",
    "ios:debug": "ENVFILE=.env.debug react-native run-ios --simulator=\"iPhone 15\"",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd app/build/outputs/apk/release && open .",
    "build:android:debug": "SET ENVFILE=.env.debug && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd app/build/outputs/apk/debug && start .",
    "build:android:staging": "SET ENVFILE=.env.staging && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd app/build/outputs/apk/debug && start .",
    "build:android:aab": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew bundleRelease && cd app/build/outputs/bundle/release && start .",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.7",
    "@hookform/resolvers": "^2.9.3",
    "@os-team/i18next-react-native-language-detector": "^1.0.17",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-clipboard/clipboard": "^1.13.2",
    "@react-native-community/cli-platform-ios": "^8.0.2",
    "@react-native-firebase/analytics": "^18.3.0",
