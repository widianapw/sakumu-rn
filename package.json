{
  "name": "reactnativestarterkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "comment": "if you are using mac device, change the command from 'SET ENVFILE=.env && react-native run android/ios' to 'ENVFILE=.env react-native run android/ios'",
    "android": "react-native run-android",
    "android:staging": "SET ENVFILE=.env.staging && react-native run-android",
    "android:debug": "SET ENVFILE=.env.debug && react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 11\"",
    "ios:staging": "SET ENVFILE=.env.staging && react-native run-ios",
    "ios:debug": "SET ENVFILE=.env.debug && react-native run-ios --simulator=\"iPhone 11\"",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd ..",
    "build:android:debug": "SET ENVFILE=.env.debug && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd ..",
    "build:android:staging": "SET ENVFILE=.env.staging && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
